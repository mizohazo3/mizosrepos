            SELECT timer_id, SUM(earned_amount) as total_earned
            FROM timer_logs
            GROUP BY timer_id
        ) tl ON t.id = tl.timer_id
     WHERE (LOWER(t.name) LIKE LOWER(:search_term)) ORDER BY t.is_running DESC, t.is_pinned DESC, t.name ASC LIMIT 50
